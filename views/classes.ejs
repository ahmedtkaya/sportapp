               </div>
            </div>
         </div>
         <!-- end header inner -->
         <!-- end header -->
         <!-- banner -->
         
      </header>
      <!-- end banner -->
      <div class="back_re">
         <div class="container">
            <div class="row">
               <div class="col-md-12">
                  <div class="title">
                       <h2>Our Classes</h2>
                  </div>
               </div>
            </div>
         </div>
      </div>
      <!-- our classes -->
      <div class="classes">
         <div class="container">
            <div class="row">
               <div class="col-sm-12">
                  <div class="titlepage">
                   
                     <h2>You see Our Classes
                     </h2>
                  </div>
               </div>
            </div>
            <div class="row">
               <div class="col-md-4 col-sm-6 d_none">
               </div>
               <% for(let i=0;i < classes.length;i++){%>
               <div class="col-md-4 col-sm-6 margin_top">
                  <div class="our_yoga">
                     <figure><img src="images/our_yoga1.png" alt="#"/></figure>
                    <a href="/classes/<%= classes[i].slug%>"><h3 style="font-size: 140%"><%=classes[i].name%></h3></a> 
                     <span><%=classes[i].description%></span>
                     <p>Coach : <%=classes[i].user.name%>....</p>
                  </div>
               </div>
               
               <div class="col-md-4 col-sm-6 d_none">
               </div>
               <%}%>
               
         </div>
      </div>
      <!-- end our classes -->
    
      <!--  footer -->
      <%- include('partials/_footer.ejs') %>
